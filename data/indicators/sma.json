{
  "id": "sma",
  "name": "SMA",
  "description": "Simple Moving Average",
  "type": "overlay",
  "pane": "main",
  "parameters": {
    "period": {
      "type": "number",
      "default": 20,
      "min": 2,
      "max": 200,
      "label": "Period"
    },
    "source": {
      "type": "select",
      "default": "close",
      "options": ["close", "open", "high", "low", "hl2", "hlc3", "ohlc4"],
      "label": "Source"
    },
    "color": {
      "type": "color",
      "default": "#2196F3",
      "label": "Color"
    }
  },
  "style": {
    "color": "#2196F3",
    "lineWidth": 2,
    "lineStyle": "solid"
  },
  "outputs": {
    "sma": {
      "name": "SMA",
      "description": "Simple Moving Average line",
      "type": "line",
      "defaultColor": "#2196F3",
      "displayName": "SMA({period})"
    }
  }
}